largest = -1
smallest = None
while True:
    num = input("Enter a number: ")
    if num == "done" :
        break
    try:
    	num1=int(num)
    except:
    	print('Invalid input')
    	continue
   
    if num1 > largest:
    	largest = num1
    else:
    	if smallest is None :
    		smallest = num1
    	elif num1 < smallest:
    		smallest = num1

    


    

print("Maximum is", largest)
print("Minimum is", smallest)
